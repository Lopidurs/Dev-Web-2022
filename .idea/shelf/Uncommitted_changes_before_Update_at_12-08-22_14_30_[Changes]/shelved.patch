+import axios from "axios";
+
+
+function Admin_cours (){
+    function sendFormCours(event){
+        const newCours = {
+            titre: event.target[0].value,
+            description: event.target[1].value,
+            heureDebut: event.target[2].value,
+            heureFin: event.target[3].value,
+            date: event.target[4].value,
+            places: event.target[5].value}
+        axios.post("/createCours", newCours, {withCredentials: true})
+        document.getElementById('succes').innerText = "Le cours à été créer avec succès!";
+    }
+    return(
+        <div className={"container"}>
+            <h1>Cours :</h1>
+            <form className="confection-form" onSubmit={sendFormCours}>
+                <div className={"titre"}>
+                    <label htmlFor="titre">titre</label>
+                    <input type="text" name="titre" autoComplete="off" required/>
+                </div>
+                <div className={"description"}>
+                    <label htmlFor="description">Description</label>
+                    <textarea name="description" rows={10} cols={50} required/>
+                </div>
+                <div className={"heureDebut"}>
+                    <label htmlFor="heureDebut">De :</label>
+                    <input type="date" name="heureDebut" required/>
+                </div>
+                <div className={"heureFin"}>
+                    <label htmlFor="heureFin">A :</label>
+                    <input type="time" name="heureFin" required/>
+                </div>
+                <div className={"date"}>
+                    <label htmlFor="date">Le :</label>
+                    <input type="time" name="date" required/>
+                </div>
+                <div className={"places"}>
+                    <label htmlFor="places">Nombres de places :</label>
+                    <input type="number" name="titre" autoComplete="off" required/>
+                </div>
+                <button type="submit" className="bouton_plein">Envoyer</button>
+                <h1 id={"succes"}></h1>
+            </form>
+        </div>
+    )
+}
+
+export default Admin_cours
